    "ng": "nx",
    "start": "nx serve",
    "start:server:dev": "ng serve api",
    "start:server:testing": "ENV=testing ng serve api",
    "seed:db": "seed -u $(grep DB_URI .env.testing | cut -d '=' -f 2-) --drop-database ./seed-data",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint && ng lint",
    "e2e": "nx e2e",
    "e2e:angular-desktop": "nx e2e angular-desktop-e2e -- --watch=true",
    "start-server-and-test:angular-desktop": "start-server-and-test start:server:testing http-get://localhost:3333/ping e2e:angular-desktop",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
