            "assets": ["apps/angular/desktop/src/favicon.ico", "apps/angular/desktop/src/assets"],
            "styles": ["apps/angular/desktop/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/angular/styles/"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular/desktop/src/environments/environment.ts",
                  "with": "apps/angular/desktop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-desktop:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-desktop:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-desktop:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular/desktop/tsconfig.app.json",
              "apps/angular/desktop/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/angular/desktop/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular/desktop/jest.config.js",
            "tsConfig": "apps/angular/desktop/tsconfig.spec.json",
            "setupFile": "apps/angular/desktop/src/test-setup.ts"
          }
        }
      }
