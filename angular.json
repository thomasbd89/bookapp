        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular/desktop",
      "sourceRoot": "apps/angular/desktop/src",
      "prefix": "bookapp",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
